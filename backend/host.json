{
  "version": "4.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5
      },
      "enableLiveMetrics": true
    }
  },
  "functionTimeout": "00:05:00",
  "http": {
    "dynamicThrottlesEnabled": true,
    "routePrefix": "api"
  }
}
