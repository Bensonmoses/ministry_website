{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "applicationInsights",
      "direction": "out",
      "name": "log"
    }
  ],
  "cors": {
    "allowedOrigins": ["*"],
    "supportCredentials": false
  }
}
